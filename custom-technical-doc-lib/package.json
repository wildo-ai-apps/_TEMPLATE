{
  "name": "@wildo-ai/custom-technical-doc-lib",
  "version": "1.0.0",
  "description": "Custom Docusaurus components library for technical documentation",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/esm/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build-custom": "pnpm clean-custom && tsup",
    "clean-custom": "rimraf dist",
    "dev-custom": "tsup --watch --watch ../custom-shared-lib/dist --watch ../../../wildo-ai/framework/saas-models/dist --watch ../../../wildo-ai/framework/zod-decorators/dist",
    "typecheck": "tsc --noEmit",
    "test": "jest --passWithNoTests",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@wildo-ai/custom-shared-lib": "workspace:*",
    "@wildo-ai/saas-models": "workspace:*",
    "@wildo-ai/zod-decorators": "workspace:*"
  },
  "devDependencies": {
    "rimraf": "*",
    "tsup": "*",
    "typescript": "*"
  },
  "peerDependencies": {
    "big.js": "*",
    "date-fns": "*",
    "lodash": "*",
    "ufo": "*"
  }
}
