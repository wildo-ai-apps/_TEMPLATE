{
  "name": "@wildo-ai/custom-shared-lib",
  "version": "1.0.0",
  "description": "Todo app specific models and types",
  "license": "UNLICENSED",
  "author": "Wildo AI",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/esm/index.d.ts",
      "import": "./dist/esm/index.js"
    }
  },
  "main": "./dist/esm/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build-custom": "pnpm clean-custom && tsup",
    "clean-custom": "rimraf dist",
    "dev-custom": "tsup --watch src --watch ../../../wildo-ai/framework/saas-models/dist/esm/.build-complete.js.trigger",
    "typecheck": "tsc --noEmit",
    "test": "jest --passWithNoTests",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@wildo-ai/saas-models": "workspace:*",
    "@wildo-ai/presets-components-models": "workspace:*",
    "@wildo-ai/zod-decorators": "workspace:*"
  },
  "peerDependencies": {
    "big.js": "*",
    "date-fns": "*",
    "inversify": "*",
    "jsonwebtoken": "*",
    "lodash": "*",
    "mime": "*",
    "reflect-metadata": "*",
    "ufo": "*",
    "uuid": "*",
    "yaml": "*",
    "zod": "*"
  },
  "devDependencies": {
    "@swc/core": "^1",
    "@types/node": "*",
    "jest": "*",
    "rimraf": "*",
    "tsup": "*",
    "typescript": "*",
    "zod": "*"
  }
}
